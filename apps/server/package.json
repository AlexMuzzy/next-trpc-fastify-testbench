        "postgres": "^3.4.7",
        "superjson": "^2.2.1",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@types/node": "^20.19.10",
        "drizzle-kit": "^0.31.4",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2"
