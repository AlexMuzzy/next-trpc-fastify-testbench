{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@fsapp/trpc": ["../../packages/trpc/src"],
      "@fsapp/trpc/*": ["../../packages/trpc/src/*"],
      "@fsapp/config": ["../../packages/config/src"],
      "@fsapp/server": ["../../apps/server/src"],
      "@fsapp/server/*": ["../../apps/server/src/*"]
    },
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
